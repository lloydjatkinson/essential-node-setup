{
  "name": "essential-node-setup",
  "version": "1.0.0",
  "description": "A basic Node and TypeScript setup",
  "main": "api.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts,.cjs,.mjs",
    "lint:fix": "eslint . --ext .ts,.cjs,.mjs --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:types": "vitest typecheck",
    "test:coverage": "vitest run --coverage",
    "test:coverage:serve": "vitest run --coverage; serve coverage",
    "dev": "node dist/backend/api.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
